programa
{
	
	funcao inicio()
	{
	inteiro x, i

	faca{
		escreva("digite o numero cinco: ")
		leia(x)
	}enquanto(x <= 0)

	para(i = 0; i <= 10; i++){
		escreva(5, " x ", i, " = ", x * i, "\n")
	}
	}
}
/* $$$ Portugol Studio $$$ 
 * 
 * Esta seção do arquivo guarda informações do Portugol Studio.
 * Você pode apagá-la se estiver utilizando outro editor.
 * 
 * @POSICAO-CURSOR = 198; 
 * @PONTOS-DE-PARADA = ;
 * @SIMBOLOS-INSPECIONADOS = ;
