programa
{
inteiro num1, num2, num3, num4, num5, num6, num7, num8, num9, num10, soma, total, resul
	
	funcao inicio()
	{
		escreva("Escreva 10 numeros: ")
		leia(num1, num2, num3, num4, num5, num6, num7, num8, num9, num10)
		limpa()
		soma = (num1+num2+num3+num4+num5+num6+num7+num8+num9+num10)
		total = (10)
		
		resul = (soma/total)

		escreva("Valor em média aritmética: ", resul)
	}
}

